{
  "name": "mac-rh",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepush": "yarn test --maxWorkers=1 && lint-staged && yarn lint",
    "build:teste": "webpack --env.NODE_ENV=development --mode development",
    "build:qa": "webpack --env.NODE_ENV=qa --mode development",
    "build:staging": "webpack --env.NODE_ENV=staging --mode production",
    "build:production": "webpack --env.NODE_ENV=production --mode production",
    "start:web": "webpack-dev-server --env.NODE_ENV=development --config ./webpack.config.js --mode development",
    "start:web:qa": "webpack-dev-server --env.NODE_ENV=local_qa --config ./webpack.config.js --mode development",
    "start:web:stgdev": "webpack-dev-server --env.NODE_ENV=local_stgdev --config ./webpack.config.js --mode development",
    "start:web:stg": "webpack-dev-server --env.NODE_ENV=local_staging --config ./webpack.config.js --mode production",
    "start:mock": "yarn mockserver && webpack-dev-server --env.NODE_ENV=mock --config ./webpack.config.js --mode development",
    "start:web:mock": "webpack-dev-server --env.NODE_ENV=mock --config ./webpack.config.js --mode development",
    "mockserver": "yarn json-server ./_mock/mock.json --routes ./_mock/routes.json -w -p 3001 -d 500 &",
    "killserver": "kill -9 $(lsof -ti:3001)",
    "start": "npm run start:web",
    "pretty": "prettier --config .prettierrc --write \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint ./src --max-warnings 100",
    "lint:fix": "eslint ./src --fix",
    "preversion": "yarn test --maxWorkers=1",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "yarn lint:fix"
    ]
  },
  "resolutions": {
    "handlebars": "4.5.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^4.3.0",
    "cpf_cnpj": "0.2.0",
    "crypto-js": "^3.1.9-1",
    "dotenv-webpack": "^1.5.7",
    "faker": "^6.4.1",
    "file-saver": "^2.0.0",
    "formik": "^1.5.8",
    "history": "^4.7.2",
    "js-base64": "^2.4.9",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.10",
    "lottie-web": "^5.4.2",
    "moment": "^2.29.4",
    "node-rsa": "^1.0.1",
    "prop-types": "15.6.1",
    "react": "16.3.1",
    "react-autosuggest": "^9.4.2",
    "react-dates": "^18.2.2",
    "react-dom": "16.3.3",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "^2.7.0",
    "react-google-recaptcha": "^2.1.0",
    "react-input-mask": "^2.0.4",
    "react-minimal-pie-chart": "^4.2.0",
    "react-modal": "^3.4.5",
    "react-native": "0.55.4",
    "react-popover": "^0.5.7",
    "react-primitives": "0.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-styled-flexboxgrid": "^3.1.0",
    "react-text-mask": "^5.4.3",
    "react-tooltip": "^4.2.6",
    "redux": "^4.0.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.2.0",
    "reduxsauce": "^1.0.1",
    "rosie": "^2.0.1",
    "rxjs": "^6.6.6",
    "solidarity": "^2.3.1",
    "styled-components": "^3.2.6",
    "telefone": "^1.2.0",
    "text-mask-addons": "^3.8.0",
    "universal-cookie": "^3.0.4",
    "yup": "0.24.1"
