    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "json-server": "^0.16.1",
    "prettier": "^1.17.1",
    "react-hot-loader": "^4.1.2",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "~16.3.1",
    "redux-mock-store": "^1.5.3",
    "string-hash": "^1.1.3",
